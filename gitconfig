[alias]
    dup = commit
    wciep = apply
    dodej = add
    lobocz = show
    pozor = diff
    
    targej - pull
    piznij = push
    fajrant = !sh -c 'git pull --rebase && git push'
    fyrlej = rebase
    
    curig = reset
    
    kiery = blame
    
    hasie = stash
    hasiewciep = stash apply
    
    kajjesmajster = !sh -c 'git shortlog HEAD..origin/master'
    
    fachmany = shortlog --numbered --summary
